<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.sql.test">

    <!--
        mybatis-config.xml의 typeAlias에 등록해두지 않았다면

        <select id="selectMember" resultType="com.example.dto.MemberVO" >
            SELECT ID, PW ,NAME FROM TEST
        </select>

        이렇게 해야 한다.
    -->
    <resultMap id="Movie" type="MovieVO">
        <result property="id" column="movie_id"/>
        <result property="name" column="movie_name"/>
        <result property="director" column="director"/>
        <result property="type" column="type"/>
    </resultMap>

    <select id="selectMovie" resultMap="Movie">
        SELECT MOVIE_NAME, DIRECTOR, TYPE FROM MOVIE
    </select>

</mapper>